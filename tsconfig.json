{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["jest", "react-native"]
  }
}